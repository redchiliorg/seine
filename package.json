{
  "name": "seine",
  "description": "Seine is a Javascript library of React components and utilities to view and edit structured content.",
  "homepage": "https://seine.wanttobe.online",
  "version": "0.3.249",
  "keywords": [
    "react",
    "richtext"
  ],
  "private": true,
  "repository": "https://github.com/mi6gan/seine",
  "license": "MIT",
  "scripts": {
    "start": "serve",
    "show": "echo ${npm_package_version}",
    "deploy": "run-s deploy:*",
    "deploy:build": "docker build -t mi6gan/seine:${npm_package_version} .",
    "deploy:version": "docker push mi6gan/seine:${npm_package_version}",
    "deploy:latest": "docker tag mi6gan/seine:${npm_package_version} mi6gan/seine:latest",
    "deploy:push": "docker push mi6gan/seine:latest",
    "release": "run-s release:*",
    "release:publish": "node scripts/publish.js",
    "release:version": "yarn version --patch",
    "release:commit": "git commit -am \"Release ${npm_package_version}\"",
    "release:push": "git push -f origin HEAD:latest",
    "test": "run-s build:* test:*",
    "test:eslint": "eslint -c .eslintrc.js --format unix packages/*/src",
    "test:jest": "jest",
    "build": "run-s build:*",
    "build:cjs": "node scripts/build.js --format=cjs",
    "build:esm": "node scripts/build.js --format=esm",
    "build:umd": "node scripts/build-workspace.js --format=umd --workspace=packages/testbox",
    "build:storybook": "yarn build-storybook --quiet --loglevel silent -o dist",
    "dev": "run-s dev:*",
    "dev:start": "start-storybook -p 3000"
  },
  "dependencies": {
    "@material-ui/core": "4.9",
    "@material-ui/icons": "4.9",
    "draft-js": "^0.11.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "serve": "^11.1.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-storyshots": "^5.3.0",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.1",
    "colorette": "^1.1.0",
    "cross-env": "^6.0.3",
    "draft-js": "^0.11.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "^15.12.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "find-cache-dir": "^3.3.0",
    "flow-bin": "^0.109.0",
    "folder-hash": "^3.3.0",
    "glob": "^7.1.4",
    "history": "^4.10.1",
    "hooks.macro": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.9.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-ms": "^6.0.0",
    "ramda": "^0.26.1",
    "react-app-polyfill": "^1.0.2",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "rollup": "^1.25.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-visualizer": "^2.5.4"
  },
  "workspaces": [
    "packages/*",
    "packages/helpers/*"
  ],
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
