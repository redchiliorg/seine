{
  "name": "seine",
  "description": "Seine is a Javascript library of React components and utilities to view and edit structured content.",
  "homepage": "https://redchiliorg.github.io/seine",
  "version": "0.0.0",
  "keywords": [
    "react",
    "richtext"
  ],
  "private": true,
  "repository": "redchiliorg/seine",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "serve",
    "build": "build-storybook -o dist",
    "release": "run-s release:*",
    "release:build": "run-p release:build:*",
    "release:build:cjs": "yarn workspaces run rollup --environment NODE_ENV:production -f cjs -c ../../rollup.config.js",
    "release:build:esm": "yarn workspaces run rollup --environment NODE_ENV:production,VISUALIZER_TEMPLATE:treemap -f esm -c ../../rollup.config.js",
    "release:publish": "node scripts/publish.js",
    "test": "run-s test:*",
    "test:build": "run-s release:build",
    "test:eslint": "eslint -c .eslintrc.js --format unix packages/*/src",
    "test:jest": "jest",
    "dev": "run-s dev:*",
    "dev:start": "start-storybook -p 9009 -s public"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "draft-js": "^0.11.0",
    "gh-pages": "^2.1.1",
    "react": "^16.8.6",
    "serve": "^11.1.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-storyshots": "^5.2.0",
    "@storybook/addons": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-react-app": "^9.0.1",
    "draft-js": "^0.11.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "^15.9.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "flow-bin": "^0.105.2",
    "glob": "^7.1.4",
    "history": "^4.10.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.9.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ramda": "^0.26.1",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-visualizer": "^2.5.4"
  },
  "workspaces": [
    "packages/core",
    "packages/ui",
    "packages/draft",
    "packages/charts",
    "packages/content",
    "packages/draft-editor",
    "packages/charts-editor",
    "packages/editor"
  ],
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
