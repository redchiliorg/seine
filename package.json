{
  "name": "seine",
  "description": "Seine is a Javascript library of React components and utilities to view and edit structured content.",
  "version": "0.0.0",
  "keywords": [
    "react",
    "richtext"
  ],
  "private": true,
  "repository": "redchiliorg/seine",
  "license": "MIT",
  "scripts": {
    "start": "serve",
    "dev": "run-s dev:*",
    "test": "run-s test:*",
    "build": "run-s build:storybook",
    "prepare": "run-s build:cjs build:esm",
    "build:cjs": "yarn workspaces run rollup --environment NODE_ENV:production -f cjs -c",
    "build:esm": "yarn workspaces run rollup --environment NODE_ENV:production -f esm -c",
    "build:storybook": "build-storybook -o dist",
    "dev:start": "start-storybook -p 9009 -s public",
    "test:eslint": "eslint --format unix ./src",
    "test:jest": "jest"
  },
  "dependencies": {
    "serve": "^11.1.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.5",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.105.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.9.0",
    "muicss": "^0.9.43",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.9.0",
    "require-context.macro": "^1.1.1",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-flow-entry": "^0.3.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "styled-components": "^4.3.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
